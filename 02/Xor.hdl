/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=a,out=na);
    Not(in=b,out=nb);
    Nand(a=a,b=nb,out=w1);
    Nand(a=na,b=b,out=w2);
    Nand(a=w1,b=w2,out=out);
}